{
  "name": "flipboxdigital/stash-pool",
  "description": "Stash Cache pool wrapper",
  "version": "1.0.0",
  "keywords": [
    "cache",
    "psr-6",
    "stash"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "tedivm/stash": "^0.14.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Stash\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Stash\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}